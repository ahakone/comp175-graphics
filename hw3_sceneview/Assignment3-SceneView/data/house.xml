<scenefile>
    <globaldata>
        <diffusecoeff v="0.5"/>
        <specularcoeff v="0.5"/>
        <ambientcoeff v="0.5"/>
    </globaldata>

    <cameradata>
        <pos x="2" y="2" z="2"/>
        <focus x="0" y="0" z="0"/>
        <up x="0" y="1" z="0"/>
        <heightangle v="45"/>
    </cameradata>

    <lightdata>
        <id v="0"/>
        <color r="1" g="1" b="1"/>
        <position x="4" y="4" z="4"/>
    </lightdata>

    <lightdata>
        <id v="1"/>
        <color r="1" g="1" b="1"/>
        <position x="-3" y="3" z="3"/>
    </lightdata>

    <lightdata>
        <id v="2"/>
        <color r="1" g="1" b="1"/>
        <position x="-3" y="3" z="-3"/>
    </lightdata>

    <lightdata>
        <id v="3"/>
        <color r="1" g="1" b="1"/>
        <position x="3" y="3" z="-3"/>
    </lightdata>

    <object type="tree" name="root">
		<transblock>
            <!-- roof -->
			<object type="tree" name="roof">
				<transblock>
                    <translate x="0.3" y=".5" z="0"/>
					<rotate x="0" y="0" z="1" angle="60"/>
					<scale x=".1" y=".75" z="1.5"/>
					<object type="primitive" name="cube">
						<diffuse r="1" g="1" b="1"/>
					</object>
				</transblock>
                <transblock>
                    <translate x="-0.3" y=".5" z="0"/>
                    <rotate x="0" y="0" z="1" angle="-60"/>
                    <scale x=".1" y=".75" z="1.5"/>
                    <object type="primitive" name="cube">
                        <diffuse r="1" g="1" b="1"/>
                    </object>
                </transblock>
			</object>
		</transblock>
		<transblock>
			<object type="tree" name="base">
				<transblock>
					<scale x=".8" y="1" z="1.3"/>
					<object type="primitive" name="cube">
						<diffuse r="1" g="0" b="0"/>
					</object>
				</transblock>
                <!-- door -->
                <transblock>
                    <translate x="0" y="-.2" z=".65"/>
                    <scale x=".5" y=".6" z=".05"/>
                    <object type="primitive" name="cube">
                        <diffuse r="0" g="0" b="0"/>
                    </object>
                </transblock>
                <transblock>
                    <translate x="0" y="-.2" z=".67"/>
                    <scale x=".4" y=".5" z=".05"/>
                    <object type="primitive" name="cube">
                        <diffuse r="1" g="1" b="1"/>
                    </object>
                </transblock>
			</object>
		</transblock>
	</object>
</scenefile> 
